<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.queqiaolove">
    <!--腾讯云直播权限-->
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.SEND_SMS"></uses-permission>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"></uses-permission>
    <uses-permission android:name="android.permission.READ_LOGS" />
    <uses-permission android:name="android.permission.RECORD_AUDIO" />
    <uses-permission android:name="android.permission.CAMERA" />

    <uses-feature android:name="android.hardware.Camera" />
    <uses-feature android:name="android.hardware.camera.autofocus" />
    <!--腾讯云直播权限-->
    <!--腾讯云IM开始-->
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.CHANGE_NETWORK_STATE" />
    <uses-permission android:name="android.permission.GET_TASKS" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.MODIFY_AUDIO_SETTINGS" />
    <uses-permission android:name="android.permission.READ_LOGS" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.RECEIVE_BOOT_COMPLETED" />
    <uses-permission android:name="android.permission.RECORD_AUDIO" />
    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <!--腾讯云IM结束-->

    <application
        android:name=".QueQiaoLoveApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_appicon"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.main.MainActivity">
            <!--<intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>-->
        </activity>
        <!--看直播-->
        <!--live观看直播页，横屏-->
        <activity
            android:name=".activity.live.horizontal.HorizontalLiveActivity"
            android:configChanges="keyboardHidden|orientation|screenSize" />
        <!--live观看直播页，竖屏-->
        <activity android:name=".activity.live.vertical.VerticalLiveActivity" />

        <!--开直播-->
        <!--pusher直播，首页-->
        <activity android:name=".activity.pusher.HomePusherActivity" />
        <!--pusher直播，直播页-->
        <activity android:name=".activity.pusher.StartPusherActivity" />
        <!--pusher直播，结束页-->
        <activity android:name=".activity.pusher.EndPusherActivity" />

        <!--登录注册-->
        <!--login，login登录页-->
        <activity android:name=".activity.login.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <!--login，register注册页-->
        <activity android:name=".activity.login.RegisterActivity" />
        <!--login，finishregister完成页-->
        <activity android:name=".activity.login.FinishRegisterActivity" />

        <!--个人中心-->
        <!--mine,我的礼物-->
        <activity android:name=".activity.mine.GiftMineActivity" />
        <!--mine,粉丝贡献-->
        <activity android:name=".activity.mine.FansConstructionMineActivity" />
        <!--mine,我的关注-->
        <activity android:name=".activity.mine.AttentionMineActivity" />
        <!--mine,我的点播 -->
        <activity android:name=".activity.mine.LiveVideoMineActivity" />
        <!--mine,我的邀请码-->
        <activity android:name=".activity.mine.InviteCodeMineActivity" />
        <!--mine,我的账户-->
        <activity android:name=".activity.mine.MyAccountMineActivity" />
        <!--mine,个人资料-->
        <activity android:name=".activity.mine.UserInfoMineActivity" />
        <!--mine,全部服务-->
        <activity android:name=".activity.mine.AllServiceActivity" />
        <!--mine,我的图片-->
        <activity android:name=".activity.mine.PhotoMineActivity" />
        <!--mine,我的视频-->
        <activity android:name=".activity.mine.VideoMineActivity" />

        <!--设置-->
        <!--mine,setting设置-->
        <activity android:name=".activity.mine.SettingMineActivity" />
        <!--mine,setting设置,通知设置-->
        <activity android:name=".activity.mine.setting.InfromSettingActivity" />
        <!--mine,setting设置,版本更新-->
        <activity android:name=".activity.mine.setting.UploadActivity" />
        <!--mine,setting设置,关于我们-->
        <activity android:name=".activity.mine.setting.AboutUsActivity" />
        <!--mine,setting设置,更改密码-->
        <activity android:name=".activity.mine.setting.ChangePwdActivity" />

        <!--member,open开通会员-->
        <activity android:name=".activity.mine.member.OpenMemberActivity" />

        <!--userinfo,个人资料-->
        <!--标签-->
        <activity android:name=".activity.mine.userinfo.LabelActivity" />
        <!--爱情宣言-->
        <activity android:name=".activity.mine.userinfo.LoveManifestoActivity" />
        <!--自我介绍-->
        <activity android:name=".activity.mine.userinfo.IntroduceActivity" />
        <!--自我介绍-->
        <activity android:name=".activity.mine.userinfo.InfoDetailActivity" />
        <!--联系方式-->
        <activity android:name=".activity.mine.userinfo.ContactWayActivity" />
        <!--联系方式,手机号码-->
        <activity android:name=".activity.mine.userinfo.contactway.ChangePhoneActivity" />
        <!--联系方式,微信号码-->
        <activity android:name=".activity.mine.userinfo.contactway.ChangeWeiChartActivity" />
        <!--联系方式,qq号码-->
        <activity android:name=".activity.mine.userinfo.contactway.ChangeQQActivity" />
        <!--教育及工作-->
        <activity android:name=".activity.mine.userinfo.EducationAndWorkActivity" />
        <!--更改内容页-->
        <activity android:name=".activity.mine.userinfo.EditContentActivity" />

        <!--正在建设页-->
        <activity android:name=".activity.login.ConstructionActivity" />

        <!--主页跳转-->
        <!--相亲活动-->
        <!--相亲活动首页-->
        <activity android:name=".activity.main.matchmaking.MatchMakingActivity" />
        <!--相亲活动详情页-->
        <activity android:name=".activity.main.matchmaking.ActivityDetailActivity" />
        <!--最热直播-->
        <activity android:name=".activity.main.PcLiveActivity" />
        <!--直播会员-->
        <activity android:name=".activity.main.PhoneLiveActivity" />

        <!--发现-->
        <!--福利，谁看过我-->
        <activity android:name=".activity.find.welfare.WhoLookedMeActivity" />
        <!--福利，我喜欢谁-->
        <activity android:name=".activity.find.welfare.ILikeWhoActivity" />
        <!--福利，会员权限详情-->
        <activity android:name=".activity.find.welfare.MemberPowerDetailActivity" />

        <!--用户信息-->
        <activity android:name=".activity.user.UserInfoActivity" />

        <!--点播页-->
        <activity
            android:name=".activity.livevideo.LiveVideoActivity"
            android:configChanges="keyboardHidden|orientation|screenSize" />


        <!--腾讯云IM开始-->
        <!--  消息收发service -->
        <service
            android:name="com.tencent.qalsdk.service.QalService"
            android:exported="false"
            android:process=":QALSERVICE"></service>
        <!--  消息收发辅助service -->
        <service
            android:name="com.tencent.qalsdk.service.QalAssistService"
            android:exported="false"
            android:process=":QALSERVICE"></service>
        <!--  离线消息广播接收器 -->
        <receiver
            android:name="com.tencent.qalsdk.QALBroadcastReceiver"
            android:exported="false">
            <intent-filter>
                <action android:name="com.tencent.qalsdk.broadcast.qal" />
            </intent-filter>
        </receiver>
        <!--  系统消息广播接收器 -->
        <receiver
            android:name="com.tencent.qalsdk.core.NetConnInfoCenter"
            android:process=":QALSERVICE">
            <intent-filter>
                <action android:name="android.intent.action.BOOT_COMPLETED" />
            </intent-filter>
            <intent-filter>
                <action android:name="android.net.conn.CONNECTIVITY_CHANGE" />
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.TIME_SET" />
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.TIMEZONE_CHANGED" />
            </intent-filter>
        </receiver>
        <!--腾讯云IM结束-->
    </application>

</manifest>